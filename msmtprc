account default
host mail.pebbleit.com
from george@pebbleit.com
user george@pebbleit.com
passwordeval gpg -d ~/.msmtp/mail.pebbleit.com.gpg
auth plain
tls on
tls_trust_file /Users/george/.mutt/certificates

account shapeshed
host smtp.gmail.com
from george@shapeshed.com
user george@shapeshed.com
passwordeval gpg -d ~/.msmtp/shapeshed.com.gpg
auth on
tls on
tls_trust_file /Users/george/.mutt/certificates
port 587

