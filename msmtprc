account shapeshed
host smtp.gmail.com
from george@shapeshed.com
user george@shapeshed.com
passwordeval gpg -d ~/.msmtp/shapeshed.com.gpg
auth on
tls on
tls_trust_file /Users/george/.mutt/certificates
port 587

