account shapeshed
host smtp.gmail.com
from george@shapeshed.com
user george@shapeshed.com
passwordeval gpg -d ~/.msmtp/shapeshed.com.gpg
passwordeval pass show email/tokens/shapeshed.com
auth on
tls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
port 587

